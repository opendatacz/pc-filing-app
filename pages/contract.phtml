    if ($conPubRes == array())
        $contractNoticePublished = false;
    else
        $contractNoticePublished = true;
    //check if award tender was already published
    $conAwTenQuery = 'SELECT ?at WHERE {
        <'.$conuri.'> <'.$GLOBALS["ns_var"]["pc"].'awardedTender> ?at . }';
