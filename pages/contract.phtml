    if ($pinPubRes == array())
        $priorNoticePublished = false;
    else
        $priorNoticePublished = true;
    //check if contractNotice was already published
    $conPubQuery = 'SELECT ?n WHERE {
        <'.$conuri.'> <'.$GLOBALS["ns_var"]["pc"].'notice> ?n .
        ?n a <'.$GLOBALS["ns_var"]["pc"].'ContractNotice> . }';
